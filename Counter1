package Counter;

	public class Counter {
		private int value;
		public int getValue() {
			return value;
		}
		public void click () {
			value = value +1;
		}
		public void unclick() {
			value=value-1;
		}
		public void reset() {
			value=0;
		}
		// part 2 
		public void clickMany (int clicksValue) {
			value = value + clicksValue;
		}
		public void unclickMany(int clicksValue) {
			value = value - clicksValue;
		}
}
